{
  "name": "iep-pubsub",
  "version": "1.0.0",
  "author": "Phil Toms <phil.toms@hotmail.co.uk>",
  "repository": {
    "type": "git",
    "url": "git://github.com/philtoms/iep-pubsub.git"
  },
  "type": "module",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "node --loader esm-fake-loader ./node_modules/.bin/ava --verbose"
  },
  "devDependencies": {
    "ava": "^3.8.2",
    "esm-fake-loader": "^1.2.7"
  },
  "ava": {
    "debug": {
      "concurrency": 1,
      "timeout": "10m",
      "nodeArguments": [
        "--no-warnings",
        "--inspect-brk=localhost:9222"
      ]
    },
    "files": [
      "**/*.test.js"
    ]
  },
  "keywords": [
    "iep",
    "es modules",
    "cache",
    "pubsub",
    "publish",
    "subscribe"
  ],
  "bugs": {
    "url": "https://github.com/philtoms/iep-pubsub/issues"
  },
  "homepage": "https://github.com/philtoms/iep-pubsub"
}
